{
  "name": "medre-be",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "node ./bin/www",
    "dev": "cross-env NODE_ENV=development nodemon ./bin/www",
    "_lint": "eslint --config ./.eslintrc.js --ext js --ext ts",
    "lint": "npm run _lint -- ./src/",
    "lint:fix": "npm run _lint -- --fix ./src/",
    "format": "prettier --write \"./**/*.{js,ts,json}\" --config ./.prettierrc",
    "prepare": "husky install"
  },
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "compression": "^1.7.4",
    "cookie-parser": "~1.4.4",
    "cors": "^2.8.5",
    "crypto-js": "^4.1.1",
    "debug": "^4.3.4",
    "dotenv": "^16.0.3",
    "express": "~4.16.1",
    "firebase-admin": "^11.4.0",
    "helmet": "^6.0.1",
    "http-errors": "~1.6.3",
    "http-status": "^1.6.2",
    "joi": "^17.7.0",
    "jsonwebtoken": "^9.0.0",
    "moment": "^2.29.4",
    "morgan": "~1.9.1",
    "multer": "^1.4.5-lts.1",
    "node-xlsx": "^0.21.0",
    "nodemailer": "^6.8.0",
    "passport": "^0.6.0",
    "passport-jwt": "^4.0.1",
    "sequelize": "^6.29.0",
    "socket.io": "^4.5.4",
    "uuid": "^9.0.0",
    "validator": "^13.9.0",
    "winston": "^3.8.2",
    "xss-clean": "^0.1.1"
  },
  "devDependencies": {
    "cross-env": "^7.0.3",
    "eslint": "^8.2.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.25.3",
    "eslint-plugin-prettier": "^4.2.1",
    "husky": "^8.0.2",
    "lint-staged": "^13.1.0",
    "nodemon": "^2.0.20",
    "prettier": "^2.8.1"
  },
  "lint-staged": {
    "src/**/*.{js,ts}": [
      "npm run lint:fix",
      "npm run format",
      "eslint --config ./.eslintrc.js --ext js --ext ts --ext jsx --ext tsx --fix"
    ]
  }
}
